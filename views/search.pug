extends layouts/main
include mixins/videoBlock

block content
    div.search__header
        h3 Searching : #{searchingBy}
    div.search__videos
        if videos.length === 0
            h5 No Videos Found
        each items in videos
            +videoBlock({
                id: items.id,
                title: items.title,
                views: items.views,
                videoFile: items.videoFile
            })